		{
			uiScript                update "ui_GetName" ;
			uiScript                updateForceStatus ;
			hide                    highlights ;
		}
		onClose
		{
			uiScript                update "ui_SetName" ;
			uiScript                updateForceStatus ;
			hide                    highlights ;
		}

